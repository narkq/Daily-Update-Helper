[wiki]
uri = https://conf.superjob.ru/rpc/xmlrpc
space = DEV
page = features-%%m-%%y
login =
password =

[general]
branch_name_regex = \*.*?([a-zA-z0-9_-]+\/[a-zA-Z0-9_-]+).*?\*
branch_name_regex_index = 1
branch_start_regex = ^# \[[^\[\]|]+\|[^\[\]|]+\]
branch_stop_regex = ^зафиксировано
editor = "%%windir%%/notepad.exe"

[git]
base_branch = master
base_branch_upstream = origin/master
tmp_branch_prefix = rebased/
rebase_flags = -p --no-ff
merge_flags = -q --no-ff
checkout_flags = -q -f

